{
  "name": "explainxkcd",
  "version": "0.1",
  "manifest_version": 2,
  "description": "Provide inline links to explainxkcd",
  "permissions": [
    "http://www.xkcd.com/", "http://explainxkcd.com/"
  ],
  "content_scripts": [
    {
      "matches": ["*://www.xkcd.com/"],
      "js": ["explainxkcd.js"]
    }
  ]
}